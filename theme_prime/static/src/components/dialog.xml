    <t t-name="theme_prime.action_buttons_group" owl="1">
        <button style="flex: 1 0 0%;" class="btn btn-primary btn-lg" t-on-click="() => this.save()">
            <t t-call="theme_prime.icon_check_circle">
                <t t-set="hw" t-value="20"/>
            </t>
            <span class="ps-3 fw-light">SAVE</span>
        </button>
        <button style="flex: 1 0 0%;" class="btn btn-default ms-3 btn-lg" t-on-click="() => this._onDiscardChange()">
            <t t-call="theme_prime.icon_cross_circle">
                <t t-set="hw" t-value="20"/>
            </t>
            <span class="ps-3 fw-light">CLOSE</span>
        </button>
    </t>
    <t t-name="theme_prime.preview_renderer" owl="1">
        <div class="row bg-white border-bottom">
            <div class="col-5 py-3 d-flex align-items-center justify-content-between">
                <img src="/theme_prime/static/src/img/snippet-config.svg" class="img img-fluid me-3" style="max-width: 200px;"/>
                <div class="p-3 tp-light-bg tp-rounded-border w-100">
                    <h3>Snippet: <t t-esc="snippetName"/>
                    </h3>
                    <h6 t-att-title="path" class="text-muted text-truncate fw-light">You're editing page: <t t-esc="path"/></h6>
                </div>
            </div>
            <div class="col-2 py-3 d-flex align-items-center justify-content-start">
                <span t-on-click="() => this.toggleViewPort('desktop')" t-attf-class="btn py-2 px-3 #{state.isMobile ? 'btn-default' : 'btn-primary'}">
                    <t t-call="theme_prime.icon_display_large">
                        <t t-set="hw" t-value="30"/>
                    </t>
                    <span class="small mt-2 fw-light d-block">DESKTOP</span>
                </span>
                <span t-on-click="() => this.toggleViewPort('mobile')" t-attf-class="py-2 ms-4 px-4 btn #{!state.isMobile ? 'btn-default' : 'btn-primary'}">
                    <t t-call="theme_prime.icon_display_mobile">
                        <t t-set="hw" t-value="30"/>
                    </t>
                    <span class="small fw-light mt-2 d-block">MOBILE</span>
                </span>
            </div>
            <div class="col-2 py-3 d-flex align-items-center justify-content-end">
                <a t-att-href="videoURL" target="_blank" class="btn btn-default ms-3 btn-lg">
                    <t t-call="theme_prime.icon_play_circle">
                        <t t-set="hw" t-value="25"/>
                    </t>
                    <span class="ps-3 fw-light">How it works?</span>
                </a>
            </div>
            <div class="col-3 py-3 d-flex align-items-center justify-content-between border-start" style="width:24%;margin-left:1%;">
                <t t-call="theme_prime.action_buttons_group"/>
            </div>
        </div>
        <div class="row g-0">
            <div class="col-9">
                <div t-if="state.isMobile and HasRecords" class="tp_preview_window sticky-top tp_mobile_mode">
                    <div class="tp_preview_container" style="top:430px !important;">
                        <div class="tp_desktop_body">
                            <img class="w-100" alt="phone" src="/theme_prime/static/src/img/browser.png"/>
                        </div>
                        <iframe t-if="HasRecords" src="/theme_prime/get_preview_body?mobile=1" t-on-load="_onPreviewLoaded" class="tp-preview-iframe h-100 w-100 pt-0 bg-white shadow-sm tp-rounded-border" t-ref="tp-preview-container"/>
                        <div class="tp_mobile_body">
                            <img alt="phone" src="/website/static/src/img/phone.png"/>
                        </div>
                    </div>
                </div>
                <div t-if="!state.isMobile and HasRecords" class="tp_preview_window sticky-top">
                    <div class="tp_preview_container shadow-sm">
                        <div class="tp_desktop_body bg-white" style="border-top-left-radius: 15px; border-top-right-radius: 15px;">
                            <img class="w-100" alt="phone" src="/theme_prime/static/src/img/browser.png"/>
                        </div>
                        <iframe t-if="HasRecords" src="/theme_prime/get_preview_body" t-on-load="_onPreviewLoaded" class="tp-preview-iframe h-100 w-100 pt-3 bg-white shadow-sm tp-rounded-border" t-ref="tp-preview-container"/>
                        <div class="tp_mobile_body">
                            <img alt="phone" src="/website/static/src/img/phone.png"/>
                        </div>
                    </div>
                </div>
                <div t-if="!HasRecords" class="my-4 text-center bg-white shadow-sm overflow-hidden" style="height:600px;border-radius:15px;">
                    <img class="w-100" alt="phone" src="/theme_prime/static/src/img/browser.png"/>
                    <img class="img my-3 img-fluid mx-auto" style="max-height:400px;" src="/theme_prime/static/src/img/canvas.png"/>
                    <h4 class="mt-3 tp-text-body">Hey, You don't need to be a designer to configure the snippet.</h4>
                    <h6 class="small d-block mb-4 fw-light tp-text-body">Just activate options from the right panel as per your need and see the magic.</h6>
                </div>
            </div>
            <div class="col-3 ps-3">
                <div class="bg-white shadow-sm position-absolute overflow-auto end-0 border-start" style="width:24%;height:calc(100% - 98px);">
                    <div class="accordion accordion-flush" id="test">
                        <t t-foreach="supportedComponents" t-as="tpComponent" t-key="tpComponent.name">
                            <div class="accordion-item border-bottom">
                                <h2 class="accordion-header">
                                    <button t-attf-class="accordion-button #{tpComponent_index !== 0 ? 'collapsed' : ''}" type="button" data-bs-toggle="collapse" t-attf-data-bs-target="##{tpComponent.name}" t-attf-aria-expanded="#{tpComponent_index !== 0 ? true : false}">
                                        <t t-if="tpComponent.name === 'TpRecordSelector'">
                                            <t t-call="theme_prime.icon_check_list">
                                                <t t-set="hw" t-value="20"/>
                                            </t>
                                            <span class="ps-3 fw-light">Selection</span>
                                        </t>
                                        <t t-if="tpComponent.name === 'TpUiComponent'">
                                            <t t-call="theme_prime.icon_brush">
                                                <t t-set="hw" t-value="20"/>
                                            </t>
                                            <span class="ps-3 fw-light">Design</span>
                                        </t>
                                    </button>
                                </h2>
                                <div t-att-id="tpComponent.name" t-attf-class="accordion-collapse bg-white p-3 collapse #{tpComponent_index === 0 ? 'show' : ''}">
                                    <t t-component="tpComponent.Component" t-props="tpComponent.props"/>
                                </div>
                            </div>
                        </t>
                    </div>
                </div>
            </div>
        </div>
    </t>

    <t t-name="theme_prime.preview_record_selector" owl="1">
        <div class="row py-3" style="background-color: #f2f7ff;">
            <t t-foreach="supportedComponents" t-as="tpComponent" t-key="tpComponent.name">
                <div class="col-12">
                    <t t-component="tpComponent.Component" t-props="tpComponent.props"/>
                </div>
            </t>
        </div>
    </t>

    <t t-name="theme_prime.snippetConfigDialog" owl="1">
        <TpConfigDialog contentClass="contentClass" size="size" footer="footer" fullscreen="true" title="title" header="false">
            <!-- [to-do] Add fonts -->
            <div t-if="templateID" style="font-family: 'Poppins';">
                <t t-call="theme_prime.preview_renderer"/>
            </div>
            <div t-else=" ">
                <t t-call="theme_prime.preview_record_selector"/>
            </div>
            <t t-if="!templateID"  t-set-slot="footer">
                <t t-call="theme_prime.action_buttons_group"/>
            </t>
        </TpConfigDialog>
    </t>
</templates>
