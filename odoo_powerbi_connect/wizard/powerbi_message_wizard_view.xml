<odoo>
    <record id="powerbi_message_wizard_form" model="ir.ui.view">
        <field name="name">powerbi.message.wizard</field>
        <field name="model">powerbi.message.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Message">
                <separator string="Message" colspan="6" />
                <field name="text" colspan="4" nolabel="1" readonly="1" widget="html" />
                <newline/>
                <separator colspan="6" />
                <footer>
                    <button special="cancel" class="oe_highlight" icon="fa-archive" string="Close" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
